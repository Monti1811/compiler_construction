int main() {
    char* str;
    str = "hello world";
    int s;
    s = 0;
    int counter;
    counter = 0;
    while (*str != 'w') {
        if (s == 0) {
            str = str + 1;
            s = 1;
        } else {
            str = 1 + str;
            s = 0;
        }
        counter = counter + 1;
    }
    str = str - counter;
    return *str;
}