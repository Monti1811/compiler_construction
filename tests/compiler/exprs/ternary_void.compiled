; ModuleID = 'tests/compiler/exprs/ternary_void.c4'
source_filename = "tests/compiler/exprs/ternary_void.c4"

define void @foo() {
entry:
  ret void
}

define i32 @main() {
entry:
  br i1 true, label %ternary-lhs, label %ternary-rhs

ternary-lhs:                                      ; preds = %entry
  br label %ternary-end

ternary-rhs:                                      ; preds = %entry
  call void @foo()
  br label %ternary-end

ternary-end:                                      ; preds = %ternary-rhs, %ternary-lhs
  br i1 false, label %ternary-lhs1, label %ternary-rhs2

ternary-lhs1:                                     ; preds = %ternary-end
  call void @foo()
  br label %ternary-end3

ternary-rhs2:                                     ; preds = %ternary-end
  br label %ternary-end3

ternary-end3:                                     ; preds = %ternary-rhs2, %ternary-lhs1
  ret i32 0
}
