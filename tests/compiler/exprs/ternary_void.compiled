; ModuleID = 'tests/compiler/exprs/ternary_void.c4'
source_filename = "tests/compiler/exprs/ternary_void.c4"

define void @foo() {
entry:
  ret void
}

define i32 @main() {
entry:
  br i1 true, label %ternary-consequence, label %ternary-alternative

ternary-consequence:                              ; preds = %entry
  br label %ternary-end

ternary-alternative:                              ; preds = %entry
  call void @foo()
  br label %ternary-end

ternary-end:                                      ; preds = %ternary-alternative, %ternary-consequence
  br i1 false, label %ternary-consequence1, label %ternary-alternative2

ternary-consequence1:                             ; preds = %ternary-end
  call void @foo()
  br label %ternary-end3

ternary-alternative2:                             ; preds = %ternary-end
  br label %ternary-end3

ternary-end3:                                     ; preds = %ternary-alternative2, %ternary-consequence1
  ret i32 0
}
