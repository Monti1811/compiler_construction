source_filename = "tests/compiler/function/label_after.c4"

define i32 @main() {
entry:
  %0 = alloca i32, align 4
  br label %label-start

label-start:                                      ; preds = %dead-after-return, %entry
  store i32 100, ptr %0, align 4
  %1 = load i32, ptr %0, align 4
  ret i32 %1

dead-after-goto:                                  ; No predecessors!
  store i32 50, ptr %0, align 4
  %2 = load i32, ptr %0, align 4
  ret i32 %2

dead-after-return:                                ; No predecessors!
  br label %label-start

dead-after-return1:                               ; No predecessors!
  ret i32 0
}
