; ModuleID = 'tests/compiler/sizeof/recursive_struct.c4'
source_filename = "tests/compiler/sizeof/recursive_struct.c4"

%struct.S = type { ptr, i32 }

define i32 @main() {
entry:
  %0 = alloca i32, align 4
  %1 = alloca %struct.S, align 8
  %2 = alloca %struct.S, align 8
  %3 = getelementptr inbounds %struct.S, ptr %2, i32 0, i32 1
  store i32 40, ptr %3, align 4
  %4 = getelementptr inbounds %struct.S, ptr %2, i32 0, i32 0
  store ptr %1, ptr %4, align 8
  %5 = getelementptr inbounds %struct.S, ptr %1, i32 0, i32 1
  store i32 2, ptr %5, align 4
  store i32 16, ptr %0, align 4
  %6 = load i32, ptr %0, align 4
  ret i32 %6

dead-after-return:                                ; No predecessors!
  ret i32 0
}
