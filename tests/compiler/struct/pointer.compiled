source_filename = "tests/compiler/struct/pointer.c4"

%struct.S = type { i32, i32, i32, ptr }

@GS = common global %struct.S zeroinitializer

define i32 @main(ptr %AS) {
entry:
  %0 = alloca i32, align 4
  %1 = alloca ptr, align 8
  store ptr %AS, ptr %1, align 8
  %2 = alloca %struct.S, align 8
  %3 = load i32, ptr @GS, align 4
  %4 = add i32 %3, 1
  %5 = getelementptr inbounds %struct.S, ptr %2, i32 0, i32 2
  %6 = load i32, ptr %5, align 4
  %7 = add i32 %4, %6
  %8 = add i32 %7, 1
  store i32 %8, ptr %0, align 4
  ret i32 0

DEAD_BLOCK:                                       ; No predecessors!
  ret i32 0
}
